{
   "version": "2.0.0",
   "tasks": [
      {
         "taskName": "Webpack",
         "command": "node_modules/.bin/webpack",
         "type": "shell",
         "problemMatcher": []
      },
      {
         "taskName": "Webpack Watch",
         "command": "node_modules/.bin/webpack --watch --display-error-details",
         "type": "shell",
         "group": "build",
         "problemMatcher": [
            "$tsc-watch"
         ],
         "presentation": {
            "echo": true,
            "reveal": "always",
            "focus": false,
            "panel": "dedicated"
         }
      }
   ]
}